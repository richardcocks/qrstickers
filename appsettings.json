{
  "DatabaseProvider": "SqlServer",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "QRStickers": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=tcp:{your-server}.database.windows.net,1433;Database=qrstickers;User ID={user};Password={password};Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  },
  "MerakiSync": {
    "EnableBackgroundSync": true,
    "PeriodicSyncIntervalHours": 6
  }
}
